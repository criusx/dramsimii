body
{
    margin-left:auto;
    margin-right:auto;
    width: 750px;
    font-family:arial, sans-serif;
}

thead
{
    background-color:rgb(20,51,102);
    height:3em;
}

.roundcont
{
    background-color:rgb(40,102,204);
    color: #fff;
    margin-top:10px;
    margin-bottom:10px;
}

.roundcont form
{
    margin-left:auto;
    margin-right:auto;
}

.roundtop
{
    background: url(../images/topRight.png) no-repeat top right;
}

.roundbottom
{
    background: url(../images/bottomRight.png) no-repeat top right;
}

img.corner
{
    width: 15px;
    height: 15px;
    border: none;
    display: block !important;
}

fieldset
{
    border:0px;
}

div#roundboxcenter
{
    text-align:center;
}

tbody#tableBody td, table#theTable thead tr th
{
    /*width:150px;*/
    width:19%;
    text-align:center;
}

a
{
    text-decoration:none;
    color:inherit;
    background-color:inherit;
}

div#credentials label, div#credentials input, div#signupDetails label, div#signupDetails input, div#signupDetails select
{
    display: block;
    float: left;
    margin-bottom: 10px;
}

div#credentials label, div#signupDetails label
{
    width:40%;
    padding-right:5%;
}

div#credentials input, div#signupDetails input, div#signupDetails select
{
    width:30%;
}

form#loginPasswordForm fieldset, form#signupForm fieldset
{
text-align:center;
}

div#credentials label, div#signupDetails label
{
    text-align: right;
    /*padding-right: 20px;*/
}

div#credentials br, div#signupDetails br
{
    clear: left;
}