<oracle-webservices xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://xmlns.oracle.com/oracleas/schema/oracle-webservices-10_0.xsd">
    <webservice-description name="COREMedDemoWS">
        <port-component name="COREMedDemoWSSoap12HttpPort">
            <operations>
                <operation name="checkInteraction" input="{http://COREMedDemo/}checkInteractionElement"/>
                <operation name="enterDrugInfo" input="{http://COREMedDemo/}enterDrugInfoElement"/>
                <operation name="enterPatientPhoto" input="{http://COREMedDemo/}enterPatientPhotoElement"/>
                <operation name="enterPatientRecord" input="{http://COREMedDemo/}enterPatientRecordElement"/>
                <operation name="getDrugInfo" input="{http://COREMedDemo/}getDrugInfoElement"/>
                <operation name="getPatientRecord" input="{http://COREMedDemo/}getPatientRecordElement"/>
                <operation name="logPatientVitals" input="{http://COREMedDemo/}logPatientVitalsElement"/>
                <operation name="registerDoseGiven" input="{http://COREMedDemo/}registerDoseGivenElement"/>
            </operations>
        </port-component>
        <port-component name="COREMedDemoWSSoapHttpPort">
            <operations>
                <operation name="checkInteraction" input="{http://COREMedDemo/}checkInteractionElement"/>
                <operation name="enterDrugInfo" input="{http://COREMedDemo/}enterDrugInfoElement"/>
                <operation name="enterPatientPhoto" input="{http://COREMedDemo/}enterPatientPhotoElement"/>
                <operation name="enterPatientRecord" input="{http://COREMedDemo/}enterPatientRecordElement"/>
                <operation name="getDrugInfo" input="{http://COREMedDemo/}getDrugInfoElement"/>
                <operation name="getPatientRecord" input="{http://COREMedDemo/}getPatientRecordElement"/>
                <operation name="logPatientVitals" input="{http://COREMedDemo/}logPatientVitalsElement"/>
                <operation name="registerDoseGiven" input="{http://COREMedDemo/}registerDoseGivenElement"/>
            </operations>
        </port-component>
    </webservice-description>
    <webservice-description name="GetDatesWS">
        <port-component name="GetDatesWSSoapHttpPort">
            <rest-support>true</rest-support>
            <operations>
                <operation name="callHome" input="{http://datespackage/}callHomeElement"/>
                <operation name="checkInteraction" input="{http://datespackage/}checkInteractionElement"/>
                <operation name="getItem" input="{http://datespackage/}getItemElement"/>
                <operation name="getPatientInfo" input="{http://datespackage/}getPatientInfoElement"/>
                <operation name="getPicture" input="{http://datespackage/}getPictureElement"/>
                <operation name="getRoute" input="{http://datespackage/}getRouteElement"/>
                <operation name="getSince" input="{http://datespackage/}getSinceElement"/>
                <operation name="getUniqueUIDs" input="{http://datespackage/}getUniqueUIDsElement"/>
                <operation name="setItem" input="{http://datespackage/}setItemElement"/>
            </operations>
        </port-component>
    </webservice-description>
    <webservice-description name="DhlDemoWS">
        <port-component name="DhlDemoWSSoapHttpPort">
            <operations>
                <operation name="getPackageInfo" input="{http://dhlDemo/}getPackageInfo"/>
                <operation name="scanPackage" input="{http://dhlDemo/}scanPackage"/>
                <operation name="setPackageInfo" input="{http://dhlDemo/}setPackageInfo"/>
            </operations>
        </port-component>
    </webservice-description>
    <webservice-description name="EvDemoWS">
        <port-component name="EvDemoWSSoapHttpPort">
            <operations>
                <operation name="addSpectrum" input="{http://evDemo/}addSpectrumElement"/>
                <operation name="getSpectrum" input="{http://evDemo/}getSpectrumElement"/>
            </operations>
        </port-component>
    </webservice-description>
    <webservice-description name="petWS">
        <port-component name="petWSSoapHttpPort">
            <operations>
                <operation name="authorize" input="{http://petPackage/types/}authorizeElement"/>
                <operation name="changePassword" input="{http://petPackage/types/}changePasswordElement"/>
                <operation name="changePasswordTagCode" input="{http://petPackage/types/}changePasswordTagCodeElement"/>
                <operation name="retrievePetInformation" input="{http://petPackage/types/}retrievePetInformationElement"/>
                <operation name="retrievePetInformationTagCode" input="{http://petPackage/types/}retrievePetInformationTagCodeElement"/>
                <operation name="updatePetInformation" input="{http://petPackage/types/}updatePetInformationElement"/>
                <operation name="updatePetInformationTagCode" input="{http://petPackage/types/}updatePetInformationTagCodeElement"/>
            </operations>
        </port-component>
    </webservice-description>
    <webservice-description name="wineWS">
        <port-component name="wineWSSoapHttpPort">
            <rest-support>true</rest-support>
            <operations>
                <operation name="enterBottleInformation" input="{http://winePackage/}enterBottleInformationElement"/>
                <operation name="getWineAccessesSince" input="{http://winePackage/}getWineAccessesSinceElement"/>
                <operation name="retrieveBottleInformation" input="{http://winePackage/}retrieveBottleInformationElement"/>
            </operations>
        </port-component>
    </webservice-description>
    <webservice-description name="AuthenticationWebService">
        <port-component name="AuthenticationWebServiceSoapHttpPort">
            <operations>
                <operation name="callHome" input="{http://authenticationPackage/}callHome"/>
                <operation name="getItem" input="{http://authenticationPackage/}getItem"/>
                <operation name="getRoute" input="{http://authenticationPackage/}getRoute"/>
                <operation name="getSince" input="{http://authenticationPackage/}getSince"/>
                <operation name="getUniqueUIDs" input="{http://authenticationPackage/}getUniqueUIDs"/>
                <operation name="setItem" input="{http://authenticationPackage/}setItem"/>
            </operations>
        </port-component>
    </webservice-description>
</oracle-webservices>
