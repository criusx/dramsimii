                    <signature-key alias="user1" key-pass=""/>
                    <encryption-key alias="user1" key-pass=""/>
                    <inbound>
                        <verify-username-token require-nonce="true"
                                               require-created="true"
                                               password-type="PLAINTEXT"/>
                        <verify-signature>
                            <signature-methods>
